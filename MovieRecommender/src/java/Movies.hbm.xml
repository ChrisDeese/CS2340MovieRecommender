<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="model.movie.Movie" table="Features">
    <meta attribute="class-description">
        this contains Movie information.
    </meta>
    <id column="movieId" name="movieId" type="int">
      <generator class="assigned"/>
    </id>
    <property column="title" name="title" type="string"/>
    <property column="id" name="Id" type="string"/>
    <property column="ratingTotal" name="ratingTotal" type="int"/>
    <property column="ratingCount" name="ratingCount" type="int"/>
    <property column="rating" name="rating" type="int"/>
    <property column="major" name="major" type="string"/>
  </class>
</hibernate-mapping>
